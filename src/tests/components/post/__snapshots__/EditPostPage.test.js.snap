exports[`should render PostForm correctly 1`] = `
<div
  className="content-container"
>
  <div
    className="page"
  >
    <div>
      <h2>
        Edit Post
      </h2>
      <Connect(PostForm)
        onSubmit={[Function]}
        post={
          Object {
            "body": "Even after 10 years we have new patch!",
            "createdAt": 1000,
            "id": "1",
            "name": "JoshTh",
            "title": "New patch",
          }
        }
      />
      <button
        className="button button--secondary"
        onClick={[Function]}
      >
        Remove
      </button>
    </div>
  </div>
</div>
`;

